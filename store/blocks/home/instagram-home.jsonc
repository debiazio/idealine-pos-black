    ]
  },
  // LINHAS DA COLUNA 3
  "flex-layout.row#linha-img-superior-coluna3": {
    "props": {
      "blockClass": "linha-img-colunas",
      "colSizing": "auto"
    },
    "children": [
      "image#postagem1-coluna3-instagram",
      "image#instagram-rodape"
    ]
  },
  "flex-layout.row#linha-img-inferior-coluna3": {
    "props": {
      "blockClass": "linha-img-colunas",
      "colSizing": "auto"
    },
    "children": [
      "image#postagem2-coluna3-instagram",
      "image#instagram-rodape"
    ]
  },
  // FIM BLOCO DAS COLUNAS
  // BLOCO DAS IMAGENS
  "image#postagem1-coluna1-instagram": {
